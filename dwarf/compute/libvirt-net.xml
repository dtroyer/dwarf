<network>
    <name>dwarf</name>
    <uuid>${uuid}</uuid>
    <forward mode='nat'>
        <nat>
            <port start='1024' end='65535'/>
        </nat>
    </forward>
    <bridge name='${bridge}' stp='on' delay='0'/>
    <ip address='${ip}' netmask='255.255.255.0'>
        <dhcp>
            <range start='${dhcp_start}' end='${dhcp_end}'/>
        </dhcp>
    </ip>
</network>
